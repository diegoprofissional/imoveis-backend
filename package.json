{
  "name": "imoveis_tdd",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "go": "cd dist/src/main && node server.js",
    "start": "ts-node --logError src/main/server.ts",
    "build": "rimraf dist && tsc && cp ./package.json ./dist/",
    "migration": "npx typeorm migration:create -n migration -d src/infra/db/postgres/migrations",
    "debug": "node --inspect=0.0.0.0:9222 --nolazy dist/main/server.js",
    "dev": "sucrase-node src/main/server.ts",
    "test": "jest --passWithNoTests --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -c jest-unit-config.js",
    "test:integration": "npm test -c jest-integration-config.js",
    "test:staged": "npm test --findRelatedTests",
    "test:coverage": "npm test --coverage",
    "test:coveralls": "npm run test:coverage && coveralls < coverage/lcov.info",
    "publicar-arquivos": "sudo rm -r /var/www/imoveis_backend/* && cp ./dist/* /var/www/imoveis_backend",
    "restart-server": "pm2 kill && pm2 start /var/www/imoveis_backend/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
